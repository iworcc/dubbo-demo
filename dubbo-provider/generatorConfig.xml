<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="context1">
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection
			connectionURL="jdbc:mysql://ioms.cn:3306/demo"
			driverClass="com.mysql.jdbc.Driver" password="root" userId="root" />
		<javaModelGenerator
			targetPackage="cn.ioms.dubbo.entity" targetProject="dubbo-provider/src/main/java" />
		<sqlMapGenerator targetPackage="mapper"
			targetProject="dubbo-provider/src/main/resources" />
		<javaClientGenerator
			targetPackage="cn.ioms.dubbo.service" targetProject="dubbo-provider/src/main/java"
			type="XMLMAPPER" />
		<table schema="User" tableName="user" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
			<!-- <columnOverride column="???" property="???" /> -->
		</table>
	</context>
</generatorConfiguration>